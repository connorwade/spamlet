{
  "name": "spamlet",
  "version": "0.1.3",
  "description": "spamlet is an efficient and simple crawler for playwright",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "files": [
    "lib/**/*",
    "readme.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "jest",
    "example:sitemap": "node examples/sitemap.js",
    "build": "node build.js && tsc",
    "build:types": "tsc"
  },
  "keywords": [
    "playwright",
    "crawler",
    "automation",
    "scraper",
    "crawling",
    "scraping"
  ],
  "author": "Connor Wade",
  "license": "Apache-2.0",
  "dependencies": {
    "playwright": "^1.35.1"
  },
  "type": "module",
  "devDependencies": {
    "@jest/globals": "^29.6.1",
    "@swc/core": "^1.3.70",
    "@swc/jest": "^0.2.27",
    "@types/node": "^20.4.4",
    "esbuild": "^0.18.11",
    "jest": "^29.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
